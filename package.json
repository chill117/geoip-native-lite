{
  "name": "geoip-native-lite",
  "version": "0.3.0",
  "description": "Super-fast IP to country lookups with minimal RAM usage.",
  "keywords": [
    "geoip",
    "geo",
    "geolookup",
    "country",
    "lookup",
    "maxmind",
    "ip",
    "ipv4",
    "ipv6"
  ],
  "main": "index.js",
  "scripts": {
    "benchmarks": "mocha test/benchmarks/ --recursive --reporter spec --ui bdd",
    "lint": "eslint --config .eslintrc.js *.js lib/*.js scripts/*.js test/**/*.js",
    "prepublish": "npm run test # runs before `npm publish`",
    "pretest": "npm run lint # runs before `npm test`",
    "test": "npm run test:unit",
    "test:unit": "mocha test/unit/ --recursive --reporter spec --ui bdd",
    "updatedata": "node scripts/data.js update"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chill117/geoip-native-lite"
  },
  "author": {
    "name": "Charles Hill",
    "email": "chill@degreesofzero.com"
  },
  "license": "MIT",
  "readmeFilename": "readme.md",
  "bugs": {
    "url": "https://github.com/chill117/geoip-native-lite/issues"
  },
  "dependencies": {
    "async": "2.6.1",
    "commander": "2.17.1",
    "ip-address": "5.8.9",
    "lazy": "1.0.11",
    "mkdirp": "0.5.1",
    "underscore": "1.9.1",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "chai": "4.1.2",
    "debug": "3.1.0"
  }
}
